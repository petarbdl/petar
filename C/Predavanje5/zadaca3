/* Да се напише програма која ќе пресметува Питагорини броеви до 100.
 *  Тоа се броеви кои го исполнуваат следниот услов a2+b2=c2 */
#include <stdio.h>
int main()
{
    int i, j, k;
    for(i=1;i<=100;i++)
    {
        for(j=1;j<=100;j++)
        {
            for(k=1;k<=100;k++)
            {
                if(i*i==j*j+k*k)
                {
                    printf("%d=%d+%d\n",i,j,k);
                }
            }
        }
    }
    return 0; /* programot zavrshil uspeshno */
}
